#!/bin/bash
function renameWorkspace {
  function handleInput {
    if [[ $1 ]]; then
      i3-msg rename workspace to "$1"
    fi
  }
  original=$( i3-msg -t get_workspaces | jq '.[] | select(.focused) | .num' )
  handleInput "$( echo "$original" | rofi -dmenu -i -p 'rename â†  ' )"
}

renameWorkspace
